#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/matsuurakazuo/Desktop/Git/cmx/publish/cmx-starter-kit/node_modules/.pnpm/cmx-sdk@0.1.19_next@16.1.6_@babel+core@7.29.0_react-dom@19.2.3_react@19.2.3__react@19.2_13c0251d0de6328a638ab8c1588c0c3b/node_modules/cmx-sdk/dist/node_modules:/Users/matsuurakazuo/Desktop/Git/cmx/publish/cmx-starter-kit/node_modules/.pnpm/cmx-sdk@0.1.19_next@16.1.6_@babel+core@7.29.0_react-dom@19.2.3_react@19.2.3__react@19.2_13c0251d0de6328a638ab8c1588c0c3b/node_modules/cmx-sdk/node_modules:/Users/matsuurakazuo/Desktop/Git/cmx/publish/cmx-starter-kit/node_modules/.pnpm/cmx-sdk@0.1.19_next@16.1.6_@babel+core@7.29.0_react-dom@19.2.3_react@19.2.3__react@19.2_13c0251d0de6328a638ab8c1588c0c3b/node_modules:/Users/matsuurakazuo/Desktop/Git/cmx/publish/cmx-starter-kit/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/matsuurakazuo/Desktop/Git/cmx/publish/cmx-starter-kit/node_modules/.pnpm/cmx-sdk@0.1.19_next@16.1.6_@babel+core@7.29.0_react-dom@19.2.3_react@19.2.3__react@19.2_13c0251d0de6328a638ab8c1588c0c3b/node_modules/cmx-sdk/dist/node_modules:/Users/matsuurakazuo/Desktop/Git/cmx/publish/cmx-starter-kit/node_modules/.pnpm/cmx-sdk@0.1.19_next@16.1.6_@babel+core@7.29.0_react-dom@19.2.3_react@19.2.3__react@19.2_13c0251d0de6328a638ab8c1588c0c3b/node_modules/cmx-sdk/node_modules:/Users/matsuurakazuo/Desktop/Git/cmx/publish/cmx-starter-kit/node_modules/.pnpm/cmx-sdk@0.1.19_next@16.1.6_@babel+core@7.29.0_react-dom@19.2.3_react@19.2.3__react@19.2_13c0251d0de6328a638ab8c1588c0c3b/node_modules:/Users/matsuurakazuo/Desktop/Git/cmx/publish/cmx-starter-kit/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../cmx-sdk/dist/cli.js" "$@"
else
  exec node  "$basedir/../cmx-sdk/dist/cli.js" "$@"
fi
